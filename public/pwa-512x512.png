<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
    <defs>
      <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
        <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
        <stop offset="100%" style="stop-color:#000000;stop-opacity:1" />
      </linearGradient>
    </defs>
    
    <!-- Background -->
    <rect width="512" height="512" fill="url(#bg)" rx="20"/>
    
    <!-- Music note -->
    <g transform="translate(256, 256)">
      <!-- Note head 1 -->
      <ellipse cx="-40" cy="30" rx="20" ry="15" fill="#ffffff"/>
      
      <!-- Stem -->
      <rect x="-24" y="30" width="8" height="90" fill="#ffffff"/>
      
      <!-- Note head 2 -->
      <ellipse cx="40" cy="-40" rx="20" ry="15" fill="#ffffff"/>
      
      <!-- Staff lines -->
      <g stroke="#4a90e2" stroke-width="2" fill="none">
        <line x1="-120" y1="-30" x2="120" y2="-30"/><line x1="-120" y1="-15" x2="120" y2="-15"/><line x1="-120" y1="0" x2="120" y2="0"/><line x1="-120" y1="15" x2="120" y2="15"/><line x1="-120" y1="30" x2="120" y2="30"/>
      </g>
    </g>
  </svg>